class Main {
    public static void main(String[] args) {
    student s1 =new student();
    s1.data("Divyesh");
    s1.data(10,20);
    }
}
class student{
    void data (int a){System.out.println(a);}
    void data (String a){System.out.println(a);}
    void data (int a, int b){System.out.println(a + b);}
}
